def heart() {
    ht="*"
    i=1
    while (i<6) {
        Printf("\n")
        i=i+1
    }

    i=1
    while (i<4) {
        j=1
        while (j<33-2*i) {
            Printf(" ")
            j=j+1
        }
        k=1
        while (k<4*i+2) {
            Printf(ht)
            k=k+1
        }
        l=1
        while (l<14-4*i) {
            Printf(" ")
            l=l+1
        }
        m=1
        while (m<4*i+2) {
            Printf(ht)
            m=m+1
        }
        Printf("\n")
        i=i+1;
    }

    for (i=1; i<4; i=i+1) {
        j=1;
        while (j<26) {
            Printf(" ");
            j=j+1;
        }
        k=1;
        while (k<30) {
            Printf(ht);
            k=k+1;
        }
        Printf("\n");
    }

    i=7;
    while (i>0) {
        j=1;
        while (j<41-2*i) {
            Printf(" ");
            j=j+1;
        }
        k=1;
        while (k<4*i) {
            Printf(ht);
            k=k+1;
        }
        Printf("\n");
        i=i-1;
    }

    i=1;
    while (i<40) {
        Printf(" ");
        i=i+1;
    }
    Printf(ht)
    i=1;
    while (i<4) {
        Printf("\n");
        i=i+1;
    }

    Printf("                                I love Meng Bao!")

    i=1;
    while (i<4) {
        Printf("\n");
        i=i+1;
    }
}

heart();